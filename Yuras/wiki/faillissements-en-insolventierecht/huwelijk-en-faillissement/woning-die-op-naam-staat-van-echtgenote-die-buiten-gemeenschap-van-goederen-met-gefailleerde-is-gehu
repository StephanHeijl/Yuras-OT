            if ($("#cookie-bar").length > 0) {
                var node = $("#cookie-bar")[0];
                node.parentNode.removeChild(node);
         		setTimeout(function() {
                    $("#adrime_pushdown_div").before(node);
		        },500);
            }
	    };
