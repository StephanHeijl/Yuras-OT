        window.reconfigbar = function() {
            if ($("#cookie-bar").length > 0) {
                var node = $("#cookie-bar")[0];
                node.parentNode.removeChild(node);
         		setTimeout(function() {
                    $("#adrime_pushdown_div").before(node);
		        },500);
            }
	    };
		setTimeout(function() {
		    if ($("#div-gpt-ad-1385543559351-1")[0].scrollHeight === 0) {
			    if ($(".add-box")[0].innerHTML.indexOf("div-gpt-ad-1385543559351-1") !== -1) {
		            $(".add-box")[0].style.display = 'none';
				}
			}
		},2000);
